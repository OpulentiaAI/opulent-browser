{
  "name": "atlas-browser-extension",
  "version": "1.0.0",
  "description": "Browser extension with sidebar chat using AI SDK",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:navigation": "tsx tests/navigation.test.ts",
    "test:e2e": "tsx tests/e2e-navigation.test.ts",
    "optimize:setup": "cd prompt-optimization && ./setup-optimization.sh",
    "optimize:planner": "cd prompt-optimization/planner && PORT=3001 npx dspyground dev",
    "optimize:evaluator": "cd prompt-optimization/evaluator && npx dspyground dev",
    "optimize:browser-automation": "cd prompt-optimization/browser-automation && npx dspyground dev",
    "optimize:gemini-computer-use": "cd prompt-optimization/gemini-computer-use && npx dspyground dev",
    "optimize:planner:run": "npx tsx prompt-optimization/run-gepa-direct.ts planner 3 10",
    "optimize:evaluator:run": "npx tsx prompt-optimization/run-gepa-direct.ts evaluator 3 8",
    "optimize:browser-automation:run": "npx tsx prompt-optimization/run-gepa-direct.ts browser-automation 3 10",
    "optimize:gemini-computer-use:run": "npx tsx prompt-optimization/run-gepa-direct.ts gemini-computer-use 2 8",
    "optimize:all": "cd prompt-optimization && ./run-optimizations-automated.sh",
    "optimize:all:direct": "npx tsx prompt-optimization/run-gepa-direct.ts",
    "optimize:fast": "npx tsx prompt-optimization/run-gepa-fast.ts",
    "optimize:extract": "npx tsx prompt-optimization/extract-best-prompts.ts",
    "optimize:apply": "npx tsx prompt-optimization/apply-optimized-prompts.ts",
    "optimize:add-osworld": "npx tsx prompt-optimization/add-osworld-samples.ts",
    "optimize:demo": "npx tsx prompt-optimization/examples/example-hybrid-workflow.ts",
    "optimize:arbor-gepa": "npx tsx prompt-optimization/examples/example-hybrid-workflow.ts",
    "optimize:samples": "echo 'Create my-tools.ts first - see QUICKSTART.md' && exit 1",
    "optimize:gepa-only": "echo 'Create gepa-only.ts first - see QUICKSTART.md' && exit 1",
    "test:navigation:dist": "node tests/navigation.dist.test.js",
    "test:ogs-streaming": "tsx test-realtime-ogs-streaming-e2e.ts",
    "test:ogs-streaming:watch": "tsx watch test-realtime-ogs-streaming-e2e.ts",
    "test:ogs-streaming:production": "npm run build && npm run test:ogs-streaming",
    "test:agentic-production": "tsx test-agentic-production-e2e.ts",
    "test:agentic-production:full": "npm run build && npm run test:agentic-production",
    "test:message-utils": "tsx tests/message-utils.test.ts"
  },
  "dependencies": {
    "@ai-sdk-tools/agents": "^1.0.0",
    "@ai-sdk-tools/artifacts": "^1.0.0",
    "@ai-sdk-tools/cache": "^1.0.0",
    "@ai-sdk-tools/devtools": "^0.6.0",
    "@ai-sdk-tools/store": "^0.16.1",
    "@ai-sdk/anthropic": "^3.0.0-beta.47",
    "@ai-sdk/gateway": "^2.0.0-beta.47",
    "@ai-sdk/google": "^3.0.0-beta.36",
    "@ai-sdk/openai": "^3.0.0-beta.45",
    "@ai-sdk/openai-compatible": "^1.0.25",
    "@ai-sdk/react": "^3.0.0-beta.88",
    "@assistant-ui/react": "^0.11.35",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "ai": "^6.0.0-beta.88",
    "braintrust": "^0.4.3",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "katex": "^0.16.25",
    "nanoid": "^5.1.6",
    "openai": "^6.7.0",
    "puppeteer": "^24.27.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "streamdown": "^1.4.0",
    "tailwindcss-animate": "^1.0.7",
    "use-stick-to-bottom": "^1.1.1",
    "workflow": "^4.0.1-beta.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.19",
    "@types/chrome": "^0.0.275",
    "@types/node": "^24.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.21",
    "chrome-remote-interface": "^0.33.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dspyground": "^0.3.10",
    "lucide-react": "^0.548.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
